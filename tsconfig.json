{
  "compilerOptions": {
    "module": "NodeNext",
    "strictPropertyInitialization": false,
    "moduleResolution": "NodeNext",
    "typeRoots": ["node_modules/@types"],
    "target": "ES6",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "strict": true,

    "esModuleInterop": true
  },
  "include": ["src/**/*"]
}
